-----------------------------------------------------------------------------------------
EWS370 dual image
Nand 清除指令: nand erase “offset"  “size" 

=>  fw1: nand erase 0x0 0x4000000

=>  fw2: nand erase 0x4000000 0x4000000

tftpboot 0x42000000 2016_01_21_16_40_ews370_371_870_871-norplusnand-loader-s.img && sf probe && imgaddr=0x42000000 && source $imgaddr:script

tftpboot 0x42000000 2016_01_21_16_40_ews370_371_870_871-ubi-root.img && nand device 0 && nand erase.chip && nand write $fileaddr 0x00000000 $filesize && nand write $fileaddr 0x4000000 $filesize

------------------------------------------------------------------------------------------------------------------
EWS1025 :
set eth1addr 00:aa:bb:cc:66;tftp 0x80060000 openwrt-ar71xx-generic-ews1025-squashfs-sysupgrade.bin&&erase 0x9f060000 +$filesize&&cp.b $fileaddr 0x9f060000 $filesize
------------------------------------------------------------------------------------------------------------------

tftp 0x08060000 openwrt-ar71xx-generic-wlr8100-squashfs-sysupgrade.bin;erase 0x9f040000 +$filesize;cp.b $fileaddr 0x9f040000 $filesize

tftpboot 0x08060000 openwrt-ar71xx-generic-ecb1200-squashfs-sysupgrade.bin;erase 0x9f040000 +$filesize;cp.b $fileaddr 0x9f050000 $filesize

------------------------------------------------------------------------------------------------------------------
EWS370:

Kernel:

set ipaddr 192.168.1.1 && tftpboot 0x42000000 2015_06_09_07_58_wlr9100-kernel.bin &&  sf probe && sf erase 0x00320000 +0x00200000 && sf write $fileaddr 0x00320000 $filesize

tftpboot 0x42000000 openwrt-ipq806x-ews370_371_670_671-kernel.bin &&  sf probe && sf erase 0x00320000 +0x00200000 && sf write $fileaddr 0x00320000 $filesize

rootfs:

tftpboot 0x42000000 openwrt-ipq806x-ews370_371_670_671-squashfs-root.img  &&   sf probe && sf erase 0x00520000 +0x01800000 && sf write $fileaddr 0x00520000 $filesize


erase 0x00520000 +0x01a00000


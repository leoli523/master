EWS380:

set serverip 192.168.1.10 && tftpboot 0x88000000 openwrt-ipq40xx-ews380ap-norplusnand-all-s.img && sf probe && imgaddr=0x88000000 && source $imgaddr:script

set serverip 192.168.1.10 && tftpboot 0x88000000 openwrt-ipq40xx-ews380ap-norplusnand-loader-s.img && sf probe && imgaddr=0x88000000 && source $imgaddr:script

set serverip 192.168.1.10 && tftpboot 0x88000000 openwrt-ipq40xx-ews380ap-norplusnand-fw-s.img && sf probe && imgaddr=0x88000000 && source $imgaddr:script

